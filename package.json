{
  "name": "proton",
  "version": "0.0.3",
  "description": "Proton Designer Rewrite",
  "main": "main.js",
  "repository": "https://github.com/ProtonDesigner/rewrite",
  "author": "TechStudent10",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/uuid": "^9.0.0",
    "concurrently": "^7.6.0",
    "electron": "22.0.0",
    "electron-builder": "^23.6.0",
    "electron-devtools-installer": "^3.2.0",
    "svg-url-loader": "^7.1.1",
    "ts-loader": "^9.4.2",
    "tsc": "^2.0.4",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@babel/core": "^7.19.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^8.2.5",
    "cross-fetch": "^3.1.5",
    "css-loader": "^6.7.1",
    "electron-updater": "^5.3.0",
    "eventsource": "^2.0.2",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "pocketbase": "^0.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "unique-names-generator": "^4.7.1",
    "uuid": "^9.0.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "scripts": {
    "electron": "electron .",
    "watch:www": "cd ui && webpack --mode development --watch",
    "build:www": "cd ui && webpack --mode production",
    "build:app": "electron-builder",
    "build": "yarn build:www && tsc && yarn build:app",
    "dev": "concurrently -n electron,watch:www \"yarn:electron*\" \"yarn:watch:www\"",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.techstudent10.proton",
    "productName": "Proton Designer",
    "mac": {
      "category": "public.app-category.graphics-design"
    },
    "dmg": {
      "icon": false
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Graphics"
    }
  }
}
